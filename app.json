{
  "expo": {
    "name": "Amovies",
    "slug": "Amovies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Logo_gif.png",
      "resizeMode": "contain",
      "backgroundColor": "#5E17EB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.mctria.amovies"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.png",
        "backgroundColor": "#444444"
      }
    },
    "web": {
      "favicon": "./assets/Logo.png"
    }
  }
}
